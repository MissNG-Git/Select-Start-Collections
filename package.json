{
  "name": "select-start-collections",
  "version": "1.0.0",
  "description": "A gamer's dream application for managing their gaming collection and sharing game creations.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/users-seedDB.js && node scripts/games-seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MissNG-Git/Select-Start-Collections.git"
  },
  "author": "MissNg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MissNG-Git/Select-Start-Collections/issues"
  },
  "homepage": "https://github.com/MissNG-Git/Select-Start-Collections#readme",
  "dependencies": {
    "@material-ui/icons": "^4.11.2",
    "bcryptjs": "^2.4.3",
    "chart.js": "^3.2.1",
    "concurrently": "^6.1.0",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.8",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-chartjs-2": "^3.0.3",
    "socket.io": "^4.1.1",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
